<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amitgupta.shinobiCharts" >
    <uses-feature android:glEsVersion="0x00020000" android:required="true"/>
    <uses-permission android:name="android.permission.SET_DEBUG_APP"></uses-permission>

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:name=".panandzoom.model.PanAndZoomApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PieSeriesActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".PanAndZoomActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".CandlestickActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".ColumnSeriesStackingActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".TickLabelFormattingActivity" android:label="shinobi Charts"></activity>
    </application>

</manifest>
